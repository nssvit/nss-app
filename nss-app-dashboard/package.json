{
  "name": "pwa-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:https": "next dev --turbopack --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:sql": "squawk --exclude=prefer-identity,prefer-bigint-over-int,require-concurrent-index-creation,require-timeout-settings,prefer-robust-stmts,adding-foreign-key-constraint,constraint-missing-not-valid src/db/migrations/*.sql",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "check": "npm run typecheck && npm run lint && npm run format:check",
    "fix": "npm run lint:fix && npm run format",
    "build:analyze": "ANALYZE=true npm run build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "predb:push": "npm run db:lint",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:pull": "drizzle-kit introspect",
    "db:check": "drizzle-kit check",
    "db:lint": "npm run lint:sql",
    "predb:setup": "npm run db:lint",
    "db:setup": "npx tsx src/db/setup.ts",
    "db:diagnose": "npx tsx src/db/diagnose.ts"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "eventemitter3": "^5.0.4",
    "framer-motion": "^12.23.24",
    "next": "15.3.8",
    "postgres": "^3.4.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.4.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "prettier": "^3.8.1",
    "squawk-cli": "^2.37.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
